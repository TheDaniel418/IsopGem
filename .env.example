# IsopGem Environment Variables

# Application environment: development, production, test
ISOPGEM_ENV=development

# Logging level: DEBUG, INFO, WARNING, ERROR, CRITICAL
ISOPGEM_LOG_LEVEL=INFO

# Optional: Custom paths for config and data
# ISOPGEM_CONFIG_DIR=./config
# ISOPGEM_DATA_DIR=./data

# Optional: Development settings
# ISOPGEM_MOCK_EXTERNAL_SERVICES=true
# ISOPGEM_PROFILE_PERFORMANCE=true 